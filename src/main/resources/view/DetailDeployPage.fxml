                                       <padding>
                                          <Insets right="10.0" />
                                       </padding>
                                    </Label>
                                    <TextField fx:id="address" layoutX="234.0" layoutY="-1.0" prefHeight="20.0" prefWidth="250.0" promptText="opc.tcp://10.28.213.213:4840" />
