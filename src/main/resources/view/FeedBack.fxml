<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.shdq.OPCUA_YML_gen_platform.controller.FeedBackController">
   <children>
      <VBox alignment="CENTER" prefHeight="300.0" prefWidth="500.0">
         <children>
            <TextArea fx:id="feedbackContent" prefHeight="200.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" />
               </VBox.margin>
            </TextArea>
            <Separator disable="true" prefHeight="5.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
               <effect>
                  <InnerShadow blurType="GAUSSIAN" color="#00000050" offsetX="2.0" offsetY="2.0" />
               </effect>
            </Separator>
            <Button fx:id="sent" defaultButton="true" disable="true" mnemonicParsing="false" style="-fx-background-color: transparent; -fx-border-width: 0; -fx-border-color: transparent;" text="发送">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Microsoft YaHei" size="15.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
